<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Articles</title>
    </head>
    <body>
        <h1>Articles</h1>
        <% @articles.each do |article|%>
